<Page x:Class="DEStore.Pages.Store"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DEStore.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Store">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="100">
            <Button Margin="5" Content="new order" x:Name="BNewOrder" Click="BNewOrder_Click"/>
            <Button Margin="5" Content="back" x:Name="Back" Click="Back_Click"/>
        </StackPanel>
    </Grid>
</Page>
