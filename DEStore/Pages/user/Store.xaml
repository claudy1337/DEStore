<Page x:Class="DEStore.Pages.user.Store"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DEStore.Pages.user"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Store">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="100">
            <Button Margin="5" Content="New order" x:Name="BNewOrder" Click="BNewOrder_Click"/>
            <Button Margin="5" Content="Basket" x:Name="Basket" Click="Basket_Click"/>
            <Button Margin="5" Content="Order" x:Name="Orders" Click="Orders_Click"/>
            <Button Margin="5" Content="Back" x:Name="Back" Click="Back_Click"/>
        </StackPanel>
    </Grid>
</Page>
