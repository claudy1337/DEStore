<Page x:Class="DEStore.Pages.Point"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DEStore.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Point">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="100">

            <Button Margin="5" Content="Warehouse" x:Name="BWarehouse" Click="BWarehouse_Click"/>
            <Button Margin="5" Content="Orders" x:Name="BOrders" Click="BOrders_Click"/>
            <Button Margin="5" Content="Back" x:Name="Back" Click="Back_Click"/>

        </StackPanel>
    </Grid>
</Page>
